void moveBotForward(void){
  frontRightMotor.forward(MAXPWM + refineRPM());
  frontLeftMotor.forward(MAXPWM - refineRPM());
  backRightMotor.forward(MAXPWM + refineRPM());
  backLeftMotor.forward(MAXPWM - refineRPM());
  }

void moveBotReverse(void){
  frontLeftMotor.backward(MAXPWM);
  frontRightMotor.backward(MAXPWM);
  backLeftMotor.backward(MAXPWM);
  backRightMotor.backward(MAXPWM);
}
  
void moveBotTorqueLeft(void){
 frontRightMotor.forward(TURNPWM);
 frontLeftMotor.backward(TURNPWM);
 backRightMotor.forward(TURNPWM);
 backLeftMotor.backward(TURNPWM);
}
void moveBotTorqueRight(void){
 frontRightMotor.forward(TURNPWM);
 frontLeftMotor.backward(TURNPWM);
 backRightMotor.forward(TURNPWM);
 backLeftMotor.backward(TURNPWM);
}

 void stopBot(void){
  frontRightMotor.brake(0);
  frontLeftMotor.brake(0);
  backRightMotor.brake(0);
  backLeftMotor.brake(0);
 }
